#include "holberton.h"
#include <stdio.h>
/**
 * print_to_98 - main function
 * @n: para
 * Return: n
 */
void print_to_98(int n)
{
	int v;

	if (n < 98)
	{
		for (v = n; v <= 98; v++)
			if (v != 98)
				printf("%v, ", v);
			else
				printf("%v\n", v);
	}
	else
	{
		for (v = n; v >= 98; v--)
			if (v != 98)
				printf("%v, ", v);
			else
				printf("%v\n", v);
	}
}
