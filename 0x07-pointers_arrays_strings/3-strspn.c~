#include "holberton.h"
/**
 * _strspn - main function
 * @s: parameter
 * @accept: parameter
 * Return: n
 */
unsigned int _strspn(char *s, char *accept)
{
	int a, b, c = 0;
	for (a = 0; s[a] != '\0'; a++)
	{
		for (b = 0; accept[b] != '\0'; b++)
		{
			if (s[a] == accept[b])
			{
				c++;
				break;
			}
		}
		if (accept[b] == '\0')
			break;
	}
	return (n);
}
